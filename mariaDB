apiVersion: apps/v1
kind: Deployment
metadata:
  name: mariadb-deployment
  namespace: ecom
  labels:
    app: mariadb
spec:
  replicas: 1
  selector:
    matchLabels:
      app: mariadb
  template:
    metadata:
      labels:
        app: mariadb
    spec:
      containers:
      - name: mariadb
        image: edowon0623/my_mariadb_server:1.0
        ports:
        - containerPort: 3306
